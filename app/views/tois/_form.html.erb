<%= form_for [@toi.grp, @toi] do |f| %>
<p>
    <%= f.label :toi_no %><br>
    <%= f.text_field :toi_no %><br>
    <%= f.label :toi_name %><br>
    <%= f.text_field :toi_name %><br>
    <%= f.label :grp_no %><br>
    <%= f.text_field :grp_no %><br>
    <% if @toi.errors.any? %>
    <%= @toi.errors.messages[:toi_name][0] %>
    <% end %>
</p>

<p>
    <%= f.submit %>
</p>

<% end %>